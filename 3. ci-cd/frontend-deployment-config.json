{
    "family": "devops-app-task",
    "memory": "2048",
    "networkMode": "awsvpc",
    "cpu": "1024",
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "containerDefinitions": [
      {
        "name": "devops-app",
        "image": "ibrolive/devops-demo:react-springboot-image-frontend",
        "cpu": 1024,
        "memory": 2048,
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "protocol": "tcp",
            "containerPort": 80
          }
        ]
      }
    ]
  }